{
    "name": "abhayagiri/abhayagiri-website",
    "type": "project",
    "description": "Abhayagiri Website",
    "license": "MIT",
    "require": {
        "php": ">=8.0.2",
        "algolia/algoliasearch-client-php": "^3.0.0",
        "algolia/scout-extended": "^2.1.0",
        "anhskohbo/no-captcha": "~3.0",
        "backpack/crud": "^5.0.0",
        "backpack/filemanager": "^2.0.1",
        "backpack/pro": "^1.6",
        "backpack/revise-operation": "^1.0",
        "barryvdh/laravel-elfinder": "^0.5.2",
        "calebporzio/parental": "^1.0",
        "doctrine/dbal": "^2.9",
        "erusev/parsedown": "^1.6",
        "fruitcake/laravel-cors": "^3.0",
        "google/apiclient": "^2.2",
        "html2text/html2text": "^4.2",
        "ifsnop/mysqldump-php": "2.*",
        "james-heinrich/getid3": "~1.9",
        "laravel/framework": "^9.0",
        "laravel/helpers": "^1.1",
        "laravel/legacy-factories": "^1.3",
        "laravel/scout": "^9.0",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "league/flysystem-aws-s3-v3": "^3.15.0",
        "league/glide": "^2.2.4",
        "league/glide-laravel": "^1.0",
        "league/html-to-markdown": "~4.8",
        "mibe/feedwriter": "^1.0",
        "michelf/php-smartypants": "^1.8",
        "mpratt/embera": "~2.0",
        "mremi/url-shortener": "~2.2",
        "nesbot/carbon": "^2.19",
        "phpspec/php-diff": "^1.1",
        "sentry/sentry-laravel": "~3.5.0",
        "spatie/laravel-google-calendar": "^3.1",
        "venturecraft/revisionable": "1.*",
        "weevers/path": "^0.2.0"
    },
    "require-dev": {
        "backpack/generators": "^3.0",
        "spatie/laravel-ignition": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.18.0",
        "fakerphp/faker": "^1.23.0",
        "laracasts/generators": "^2.0.2",
        "laravel/homestead": "^14.4.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.0",
        "simplepie/simplepie": "~1.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "ocramius/package-versions": true,
            "php-http/discovery": false
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.backpackforlaravel.com/"
        }
    ]
}
