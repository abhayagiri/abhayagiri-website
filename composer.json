{
    "name": "abhayagiri/abhayagiri-website",
    "description": "Abhayagiri Website",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.3.0",
        "anhskohbo/no-captcha": "~3.0",
        "backpack/base": "^0.8.0",
        "backpack/crud": "^3.3.0",
        "barryvdh/laravel-elfinder": "~0.3",
        "erusev/parsedown": "^1.6",
        "ezyang/htmlpurifier": "~4.10",
        "fideloper/proxy": "^4.0",
        "ifsnop/mysqldump-php": "2.*",
        "james-heinrich/getid3": "~1.9",
        "laravel/framework": "5.6.*",
        "laravel/socialite": "3.*",
        "laravel/tinker": "^1.0",
        "league/html-to-markdown": "~4.8",
        "mibe/feedwriter": "^1.0",
        "michelf/php-smartypants": "^1.8",
        "mpratt/embera": "^1.9",
        "mremi/url-shortener": "~2.2",
        "nesbot/carbon": "^1.21",
        "phpfalcon/laravel-clear-orders-by": "^1.0",
        "phpspec/php-diff": "^1.1",
        "sentry/sentry-laravel": "~1.0",
        "symfony/process": "^4.0",
        "venturecraft/revisionable": "1.*",
        "weevers/path": "^0.2.0"
    },
    "require-dev": {
        "backpack/generators": "~1.1",
        "codeception/codeception": "~2.5",
        "codeception/mockery-module": "~0.3",
        "filp/whoops": "~2.3",
        "flow/jsonpath": "~0.4",
        "fzaninotto/faker": "~1.8",
        "mockery/mockery": "~1.2",
        "neronmoon/scriptsdev": "~0.1",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "simplepie/simplepie": "~1.5",
        "symfony/css-selector": "^4.0",
        "symfony/dom-crawler": "^4.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
         ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.3.6"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
