{
    "name": "abhayagiri/website",
    "description": "Abhayagiri Website",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "mibe/feedwriter": "^1.0",
        "league/oauth2-google": "^1.0",
        "ezyang/htmlpurifier": "^4.8",
        "nesbot/carbon": "^1.21",
        "anhskohbo/no-captcha": "^2.1",
        "symfony/process": "3.1.*",
        "ifsnop/mysqldump-php": "2.*",
        "sentry/sentry-laravel": "^0.3.0",
        "weevers/path": "^0.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "codeception/codeception": "^2.2.4",
        "fzaninotto/faker": "~1.4",
        "jakoch/phantomjs-installer": "2.1.1-p06",
        "site5/phantoman": "^1.1.6",
        "mockery/mockery": "^0.9.5",
        "simplepie/simplepie": "^1.4",
        "neronmoon/scriptsdev": "^0.1.0",
        "codeception/mockery-module": "^0.2.2",
        "deployer/deployer": "^5.1.1",
        "flow/jsonpath": "^0.3.4"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "App\\Console\\Composer::artisanKeyGenerate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "App\\Console\\Composer::artisanOptimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "App\\Console\\Composer::artisanOptimize"
        ]
    },
    "scripts-dev": {
        "post-install-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ],
        "post-update-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
