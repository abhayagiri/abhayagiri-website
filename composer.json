{
    "name": "abhayagiri/abhayagiri-website",
    "description": "Abhayagiri Website",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "mibe/feedwriter": "^1.0",
        "league/oauth2-google": "^1.0",
        "ezyang/htmlpurifier": "^4.8",
        "nesbot/carbon": "^1.21",
        "anhskohbo/no-captcha": "^2.1",
        "symfony/process": "~3.3",
        "ifsnop/mysqldump-php": "2.*",
        "sentry/sentry-laravel": "^0.3.0",
        "weevers/path": "^0.2.0",
        "laravel/socialite": "3.*",
        "backpack/base": "^0.7.21",
        "backpack/crud": "^3.2",
        "league/html-to-markdown": "^4.4",
        "erusev/parsedown": "^1.6",
        "backpack/settings": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "codeception/codeception": "^2.2.4",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "simplepie/simplepie": "^1.4",
        "neronmoon/scriptsdev": "^0.1.0",
        "codeception/mockery-module": "^0.2.2",
        "deployer/deployer": "^5.1.1",
        "flow/jsonpath": "^0.3.4",
        "backpack/generators": "^1.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
